{
  "name": "base_setup",
  "private": true,
  "version": "1.0.0",
  "description": "setup for tasks in React2024Q3 in RS School",
  "type": "module",
  "scripts": {
    "start": "remix-serve dist/server/index.js",
    "dev": "remix vite:dev",
    "build": "tsc && remix vite:build",
    "lint": "eslint ./src --ext ts,tsx",
    "lint:fix": "eslint ./src --ext ts,tsx --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "stylelint": "stylelint ./src/**/*.{css,scss} --quiet-deprecation-warnings",
    "stylelint:fix": "stylelint ./src/**/*.{css,scss} --fix --quiet-deprecation-warnings",
    "test": "vitest run --coverage",
    "prepare": "husky",
    "deploy": "npm run build && npx gh-pages -d dist -e remix-ssr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VadimKol/rs-react.git"
  },
  "license": "ISC",
  "homepage": "https://github.com/VadimKol/rs-react#readme",
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "not < 0.25%"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint",
    "*.{scss,css}": "stylelint",
    "*.html": "prettier --check"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@reduxjs/toolkit": "^2.2.6",
    "@remix-run/node": "^2.11.1",
    "@remix-run/react": "^2.11.1",
    "@remix-run/serve": "^2.11.1",
    "@remix-run/testing": "^2.11.1",
    "classnames": "^2.5.1",
    "isbot": "^4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.2",
    "rickmortyapi": "^2.3.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.11.1",
    "@testing-library/jest-dom": "^6.4.3",
    "@testing-library/react": "^15.0.6",
    "@testing-library/user-event": "^14.5.2",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^2.0.5",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-compiler": "^0.0.0-experimental-0998c1e-20240625",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "gh-pages": "^6.1.1",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^24.1.1",
    "lint-staged": "^15.2.2",
    "path": "^0.12.7",
    "postcss": "^8.4.35",
    "postcss-media-minmax": "^5.0.0",
    "prettier": "^3.2.5",
    "sass": "^1.75.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-order": "^6.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-checker": "0.6.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.5"
  }
}
